FRAMEWORK_SEARCH_PATHS = $(inherited)  "$(PLATFORM_DIR)/Developer/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/LayoutTest" "${PODS_ROOT}/Headers/Public/LayoutTestBase"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/LayoutTest" -isystem "${PODS_ROOT}/Headers/Public/LayoutTestBase"
OTHER_LDFLAGS = $(inherited) -ObjC -l"LayoutTest" -l"LayoutTestBase" -framework "Foundation" -framework "UIKit" -framework "XCTest"
PODS_ROOT = ${SRCROOT}/Pods